<div class="chat_message_time">
    <%= timeStamp %>
</div>
<table>
	<tbody>
		<tr>
			<% if (fromUser === true) { %>
				<td class="message">
					<div class="message_text"><%= messageBody %></div>
				</td>
                <td class="time"><%= (userName || '') + ' ' + toTimeString %></td>
			<% } else { %>
                <td class="time"><%= saslName + ' ' + toTimeString %></td>
				<td class="message">
					<div class="message_text"><%= messageBody %></div>
				</td>
			<% } %>
		</tr>
	</tbody>
</table>
