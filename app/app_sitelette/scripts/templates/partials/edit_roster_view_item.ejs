<% if (editable) { %>
    <a class="cmntyex-remove-list-item ui-btn ui-shadow ui-corner-all
        <% if (selected) { %>ui-icon-delete<%} else {%>ui-icon-none<% } %>
        ui-btn-icon-left">
   <span>    <%=  quantity  %> x </span> <span style="padding-left:100px;" ><%=  displayText %></span>
    </a>
<% } else { %>
    <li class="cmntyex-remove-list-item">
  <span>     <%=  quantity %> x </span> <span style="padding-left:100px;" > <%=  displayText %>  </span>
    </li>
<% } %>
