<section id="catalog-tabs">
  <!-- render group tabs here -->
</section>
<div class="tabcontent">
  <% if(catalogId) { %>
    <div class="header">
      <div class="back_to_catalog_btn">
        <i class="fa fa-arrow-left" aria-hidden="true"></i>
        <span>Back to catalog</span>
      </div>
    </div>
  <% } %>
  <% if (!catalogId && !filled) { %>
    <div class="header">
        <div class="empty-catalog">
          empty catalog
        </div>
    </div>
  <% } %>
  <div class="catalog-container" id="catalog-group">
    <!-- render group items here -->
  </div>
</div>